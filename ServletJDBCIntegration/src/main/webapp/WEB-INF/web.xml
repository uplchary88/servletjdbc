<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>ServletJDBCIntegration</display-name>
  <welcome-file-list>
 <welcome-file>Login.jsp</welcome-file>
 </welcome-file-list>
 <servlet>
 <servlet-name>EmpReg</servlet-name>
 <servlet-class>com.app.servlet.EmployeeRegisterServlet</servlet-class>
 </servlet>
 <servlet-mapping>
 <servlet-name>EmpReg</servlet-name>
 <url-pattern>/insert</url-pattern>
 </servlet-mapping>
 
 <servlet>
 <servlet-name>EmpLogin</servlet-name>
 <servlet-class>com.app.servlet.LoginServlet</servlet-class>
 </servlet>
 <servlet-mapping>
 <servlet-name>EmpLogin</servlet-name>
 <url-pattern>/login</url-pattern>
 </servlet-mapping>
 
 <servlet>
 <servlet-name>empdelete</servlet-name>
 <servlet-class>com.app.servlet.DeleteServlet</servlet-class>
 </servlet>
 <servlet-mapping>
 <servlet-name>empdelete</servlet-name>
 <url-pattern>/delete</url-pattern>
 </servlet-mapping>
 
 
 <servlet>
 <servlet-name>searchdob</servlet-name>
 <servlet-class>com.app.servlet.SearchByDobServlet</servlet-class>
 </servlet>
 <servlet-mapping>
 <servlet-name>searchdob</servlet-name>
 <url-pattern>/searchbydob</url-pattern>
 </servlet-mapping>
 
</web-app>